<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="200dp"
                android:height="40dp" />
            <solid android:color="#6acf71" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#73e67b" />
        </shape>
    </item>
    <item
        android:width="70dp"
        android:height="50dp"
        android:gravity="center"
        android:drawable="@drawable/skip">
    </item>
</layer-list>